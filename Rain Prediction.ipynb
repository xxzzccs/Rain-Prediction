   "source": [
    "### <ul><li>Gathering Historic Data from $1^{st}$ Jan, 2022 to October $2^{nd}$, 2025</li></ul>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "fb235ab8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Latitude: 13.0\n",
      "Longitude: 77.5\n",
      "Elevation: 883.0\n"
